@{
    ViewBag.Title = "Forgot Password Confirmation";
}


<br /><br /><br />
<div class="row">
    <div class="col-xs-12 col-sm-8 col-md-4 col-lg-4 col-center-block">
        <div class="rsSignInCard center-block">
            <h3>@ViewBag.Title</h3>
            <p>
                Please check your email to reset your password.
            </p>
            <a href="@Url.Action("Login", "Account")" class="btn btn-group-sm btn-info">Back To Login Page</a>
        </div>
    </div>
</div>
